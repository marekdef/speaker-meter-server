<h1>Agenda</h1>

<table border='1'>
  <tr>
    <th>time/venue</th>
    <% @venues.each { |venue| %>
      <th><%= venue.venue %></th>
    <% } %>
  </tr>
  <% @agenda.each do |time, speakers| %>
  <tr>
    <td>
      <%= TimeSlot.find(time).start_time %>
      </br>
      <%= TimeSlot.find(time).end_time %>
    </td>
    <% speakers.each do |speaker| %>
      <td>
        <% if speaker.length != 0 %>
          <%= speaker[0].presentation %>
        <% end %>
      </td>
    <% end %>
  </tr>
  <% end %>
</table>
